<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
          <record id="author1" model="res.partner">
              <field name="name">JJ Doc martin something</field>
              <field name="is_author">True</field>
          </record>

          <record id="editor1" model="library.editor">
              <field name="name">Anne Groell</field>
          </record>

          <record id="book1" model="library.book">
              <field name="title">A game of ice and fire</field>
              <field name="authors" eval="[ref('author1')]"/>
              <field name="year_of_edition">2010</field>
              <field name="isbn">12-34-5678-9123</field>
              <field name="language">1</field>
              <field name="editor" ref="editor1"/>
          </record>
    </data>
</odoo>